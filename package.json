{
  "name": "@salesforce/plugin-login",
  "description": "Login and logout commands for sf",
  "version": "1.0.6",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^1.0.4",
    "@salesforce/core": "3.7.2",
    "@salesforce/sf-plugins-core": "^1.0.4",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.0",
    "open": "^8.0.6",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.1.1",
    "@salesforce/cli-plugins-testkit": "^1.4.11",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "^0.9.18",
    "@salesforce/plugin-alias": "^1.1.22",
    "@salesforce/plugin-command-reference": "^2.1.1",
    "@salesforce/plugin-config": "^2.2.9",
    "@salesforce/plugin-env": "1.0.3",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.3.21",
    "@types/inquirer": "^8.1.3",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "cli-ux": "^5.6.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.2.2",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "oclif": "^2.0.0-main.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "shelljs": "^0.8.4",
    "shx": "0.3.3",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "@oclif/parser": "3.8.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/schemas"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-login",
  "keywords": [
    "force",
    "salesforce",
    "sf",
    "sf login",
    "sf logout",
    "sf-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "topicSeparator": " ",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference",
      "@salesforce/plugin-alias",
      "@salesforce/plugin-config",
      "@salesforce/plugin-env"
    ],
    "hooks": {
      "sf:logout": "./lib/hooks/logout"
    },
    "topics": {
      "login": {
        "description": "Commands to log in to an environment.",
        "subtopics": {
          "org": {
            "description": "Commands to log in to a Salesforce org."
          }
        }
      },
      "logout": {
        "description": "Commands to log out of an environment."
      }
    }
  },
  "repository": "salesforcecli/plugin-login",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:json-schema && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:json-schema": "./bin/dev schema:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif readme"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sf-husky-pre-commit",
      "commit-msg": "sf-husky-commit-msg",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
